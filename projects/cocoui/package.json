{
  "name": "cocoui",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/start.js",
  "build": {
    "appId": "cocobot.coocui",
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "bootstrap": "^4.4.0",
    "electron": "^7.1.2",
    "electron-is-dev": "^1.1.0",
    "electron-rebuild": "^1.8.8",
    "flot": "^4.2.0",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.4.1",
    "pegjs": "^0.10.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.4.1",
    "reactstrap": "^8.1.1",
    "redux": "^4.0.4",
    "serialport": "^8.0.5"
  },
  "scripts": {
    "electron": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "cocoui": "cross-env NODE_ENV=production nodemon --exec \"\"electron .\"\"",
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "install": "electron-rebuild"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.1"
  }
}
